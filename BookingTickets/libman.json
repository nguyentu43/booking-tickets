{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "summernote@0.8.18",
      "destination": "wwwroot/lib/summernote/"
    },
    {
      "library": "jquery-datetimepicker@2.5.20",
      "destination": "wwwroot/lib/jquery-datetimepicker/"
    },
    {
      "library": "moment.js@2.29.1",
      "destination": "wwwroot/lib/moment.js/"
    },
    {
      "library": "bootstrap@4.5.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "css/bootstrap.min.css",
        "js/bootstrap.bundle.min.js"
      ]
    },
    {
      "provider": "filesystem",
      "library": "wwwroot/lib/datatables.net/",
      "destination": "wwwroot/datatables/"
    },
    {
      "library": "slick-carousel@1.8.1",
      "destination": "wwwroot/lib/slick-carousel/"
    }
  ]
}